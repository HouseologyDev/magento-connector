{
  "name": "b2bapp/update-handler",
  "description": "Update Handler module for Magento 2",
  "type": "magento2-module",
  "license": [
    "proprietary"
  ],
  "require": {
    "php": "~5.5.0|~5.6.0|~7.0.0|~7.1.0",
    "magento/framework": "100.*.*"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "Magento\\SampleNewPage\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://git.houseology.com/shree/Intrepid.git",
      "options": {
        "ssl": {
          "verify_peer": false,
          "allow_self_signed": true
        }
      }
    }
  ],
  "extra": {
    "map": [
      [
        "*",
        "B2bapp/UpdateHandler"
      ]
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}