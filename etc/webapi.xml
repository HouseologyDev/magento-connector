<?xml version="1.0"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route url="/V1/b2bapp/updatedentities" method="GET">
        <service class="B2bapp\UpdateHandler\Api\UpdateHandlerInterface" method="get"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>

    <route url="/V1/b2bapp/setstatus" method="POST">
        <service class="B2bapp\UpdateHandler\Api\UpdateHandlerInterface" method="updatestatus"/>
        <resources>
            <resource ref="anonymous"/>
        </resources>
    </route>
</routes>